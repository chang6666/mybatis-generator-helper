{
  "name": "mybatis-generator-helper",
  "displayName": "mybatis-generator-helper",
  "description": "mybatis generator插件",
  "version": "0.0.4",
  "publisher": "changningbo",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mybatis.generator"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mybatis.generate",
        "title": "MyBatis: Generate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "mysql2": "^3.14.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chang6666/mybatis-generator-helper.git"
  }
}
